<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="general">
        <field name="custom_link" sortOrder="40" formElement="input">
            <settings>
				<notice translate="true">
					Example values of this field: "http://www.example.com" (absolute link) or "contact/" (relative link - without leading slash). If blank, default category link will work.
                </notice>
                <dataType>string</dataType>
                <label translate="true">Custom Link</label>
            </settings>
        </field>
    </fieldset>
</form>
